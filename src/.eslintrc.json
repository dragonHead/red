{
  "env": {
    "browser": true,
    "es2022": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "prettier/prettier": [
      "error",
      {
        "htmlWhitespaceSensitivity": "ignore"
      }
    ]
  }
}
